                assign target_collection = collection
                break
              endif
            endfor
          endif
        endif
        paginate target_collection.products by 4
          assign products = target_collection.products  
        endpaginate
      else
        if productSrc == 'RelatedProduct'
        assign relatedTag = ''
        assign assignProduct = product
        assign idAssignProduct = assignProduct.id
        assign allProducts = collections.all.products
        assign handleAssignProduct = assignProduct.handle

          if relatedTag == 'collection'
            assign hasRelatedExclude = 
            if hasRelatedExclude
              assign relatedExclude = ''
            else
              assign relatedExclude = ''
            endif
            assign arrayRelatedExclude = relatedExclude | split: ","
            for collection in collections
            assign collectionId = collection.id
            assign isInCollection = false
            assign collectionsAssignProduct = assignProduct.collections
              for item in collectionsAssignProduct
                unless arrayRelatedExclude contains item.handle
                  if item.id == collectionId 
                    assign isInCollection = true
                  endif
                endunless
              endfor
              if isInCollection
                for item in allProducts
                assign handle = item.handle
                assign productF = all_products[handle] | sort  
                      for product in collection.products
                            assign productCollectionhandle = product.handle
                            
                              if productCollectionhandle == handle and handleAssignProduct != handle
                                    assign products = products | concat: productF
                              endif
                            
                      endfor
                endfor
              endif
            endfor
            assign products = products | uniq
          endif

          if relatedTag == 'vendor'
            assign vendorAssignProduct = assignProduct.vendor
            assign productFilter = allProducts | where: 'vendor', vendorAssignProduct
            for item in productFilter
              assign itemHandle = item.handle
              assign productF = all_products[itemHandle] | sort  
              if itemHandle != handleAssignProduct
                assign products = products | concat: productF
              endif
            endfor
          endif

          if relatedTag == 'production-type'
            assign typeAssignProduct = assignProduct.type
            assign productFilter = allProducts | where: 'type', typeAssignProduct
            for item in productFilter
              assign itemHandle = item.handle
              assign productF = all_products[itemHandle] | sort  
              if itemHandle != handleAssignProduct
                assign products = products | concat: productF
              endif
            endfor
          endif

          if relatedTag == 'tags'
            assign tagsAssignProduct = assignProduct.tags 
            assign hasRelatedExclude = 
            if hasRelatedExclude
              assign relatedExclude = ''
            else
              assign relatedExclude = ''
            endif
            assign arrayRelatedExclude = relatedExclude | split: ","
               for item in allProducts 
